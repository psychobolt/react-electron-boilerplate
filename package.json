    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "sequelize-mock": "^0.10.2",
    "source-map-loader": "^0.2.4",
    "spectron": "10.0.1",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terminate": "^2.1.2",
