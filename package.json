{
  "name": "react-electron-boilerplate",
  "version": "0.0.1",
  "description": "The essential framework for React-Electron development.",
  "main": "src/.build/main.bundle.js",
  "scripts": {
    "postinstall": "rimraf flow-typed/npm && npm run flow-typed-install:dev && npm run flow-typed-install:app && electron-builder install-app-deps",
    "flow-typed-install:dev": "flow-typed update -s",
    "flow-typed-install:app": "cd src/ && flow-typed update -s -p ../ || true",
    "build.app:dev": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack-dev-server --config webpack.app.babel.js",
    "build.app:prod": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack --config webpack.app.babel.js",
    "build.main:dev": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack -w --config webpack.main.babel.js",
    "build.main:prod": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack --config webpack.main.babel.js",
    "build:dev": "concurrently -p {name} -c pink,orange,white npm:build.app:dev npm:build.main:dev",
    "build:prod": "npm run build.app:prod && npm run build.main:prod",
    "build:dist": "electron-builder",
    "build": "npm run build:prod && npm run build:dist",
    "start": "npm run dev",
    "stop": "cross-env BABEL_ENV=commonjs babel-node scripts/kill.js",
    "dev": "npm run build:dev -- npm:app",
    "prod": "npm run build:prod && electron .",
    "app": "cross-env BABEL_ENV=commonjs babel-node scripts/start.js",
    "test": "cross-env BABEL_ENV=test jest",
    "test-e2e": "ava",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint ./src/**/*.js",
    "lint:js": "eslint src e2e *.js",
    "flow-typed": "flow-typed"
  },
  "build": {
    "linux": {
      "target": [
        "AppImage",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "app": "src"
    }
  },
  "ava": {
    "files": [
      "e2e/**/*.js",
      "!src/**/*"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test-config.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/(main|menu|globals).js",
      "!**/.build/**",
      "!src/**/(index|reducer).js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.mock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/style.mock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psychobolt/react-electron-boilerplate.git"
  },
  "author": "psychobolt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psychobolt/react-electron-boilerplate/issues"
  },
  "homepage": "https://github.com/psychobolt/react-electron-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.6.2",
    "ava": "^2.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cash-true": "^0.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.6.1",
    "concurrently": "^4.1.2",
    "core-js": "^3.2.1",
    "cross-env": "^6.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.2.0",
    "electron": "6.0.10",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-babel-module": "5.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "find-process": "^1.4.2",
    "flow-bin": "0.108.0",
    "flow-typed": "2.6.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-hot-loader": "^4.12.11",
    "react-test-renderer": "^16.9.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.2.0",
    "source-map-loader": "^0.2.4",
    "spectron": "8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "terminate": "^2.1.2",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "connected-react-router": "^6.5.2",
    "electron-log": "^3.0.8",
    "electron-redux": "^1.4.0",
    "electron-store": "^5.0.0",
    "history": "^4.9.0",
    "jquery": "^3.4.1",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-undo": "^1.0.0-beta9-9-7",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "typeface-roboto": "^0.0.75",
    "typeface-roboto-mono": "^0.0.75",
    "xel": "^0.0.210"
  }
}
